@model Exam_answerWeb.Models.BaseViewModel

<!doctype html>
<html amp lang="en">
<head>
    <meta charset="utf-8">
    <title>@Model?.PageTitle</title>
    <link rel="canonical" href="@Model?.PageCanonicalUrl">
    <meta name="viewport" content="width=device-width,minimum-scale=1,initial-scale=1">

    <link href="https://fonts.googleapis.com/css?family=Inconsolata:700" rel="stylesheet">

    <partial name="_BoilerplateCssPartial.cshtml" />
    <partial name="_AmpCustomCssPartial.cshtml" />
    <script async="" src="https://cdn.ampproject.org/v0.js"></script>

    <script async custom-element="amp-accordion" src="https://cdn.ampproject.org/v0/amp-accordion-0.1.js"></script>
    <script custom-element="amp-sidebar" src="https://cdn.ampproject.org/v0/amp-sidebar-0.1.js" async=""></script>
    <script custom-element="amp-list" src="https://cdn.ampproject.org/v0/amp-list-0.1.js" async=""></script>
    <script custom-element="amp-bind" src="https://cdn.ampproject.org/v0/amp-bind-0.1.js" async=""></script>


</head>
<body>
    <header class="ampstart-headerbar fixed flex justify-start items-center top-0 left-0 right-0 pl2 pr4 ">
        <div role="button" aria-label="open sidebar" on="tap:header-sidebar.toggle" tabindex="0" class="ampstart-navbar-trigger md-hide lg-hide pr2  ">
            ☰
        </div>
        <a href="land-see.amp.html" class="text-decoration-none inline-block mx-auto ampstart-headerbar-home-link  ">
            <div class="ampstart-headerbar-title mx-auto ">Exam Answer</div>
        </a>
        <nav class="ampstart-headerbar-nav ampstart-nav  xs-hide sm-hide">
            <ul class="list-reset center m0 p0 flex justify-center nowrap">
                <li class="ampstart-nav-item "><a href="@($"{Model?.PageCanonicalUrl}/microsoft")" class="text-decoration-none block">Microsoft</a></li>
                <li class="ampstart-nav-item "><a href="@($"{Model?.PageCanonicalUrl}/salesforce")" class="text-decoration-none block">Salesforce</a></li>
                <li class="ampstart-nav-item "><a href="@($"{Model?.PageCanonicalUrl}")" class="text-decoration-none block">About</a></li>
                <li class="ampstart-nav-item "><a href="@($"{Model?.PageCanonicalUrl}")" class="text-decoration-none block">Contact</a></li>
            </ul>
        </nav>
    </header>

    <amp-sidebar id="header-sidebar" class="ampstart-sidebar px3  md-hide lg-hide " layout="nodisplay">
        <div class="flex justify-start items-center ampstart-sidebar-header">
            <div role="button" aria-label="close sidebar" on="tap:header-sidebar.toggle" tabindex="0" class="ampstart-navbar-trigger items-start">✕</div>
        </div>
        <nav class="ampstart-sidebar-nav ampstart-nav">
            <ul class="list-reset m0 p0 ampstart-label">
                <li class="ampstart-nav-item land-see-sidebar-nav-item ampstart-title-sm bold"><a class="ampstart-nav-link" href="@($"{Model?.PageCanonicalUrl}/microsoft")">Microsoft</a></li>
                <li class="ampstart-nav-item ampstart-nav-dropdown relative ">

                    <!-- Start Dropdown-inline -->
                    <amp-accordion layout="container" disable-session-states="" class="ampstart-dropdown">
                        <section expanded="">
                            <header>Microsoft</header>
                            <ul class="ampstart-dropdown-items list-reset m0 p0">
                                <li class="ampstart-dropdown-item"><a href="@($"{Model?.PageCanonicalUrl}/microsoft/az-103")" class="text-decoration-none">AZ-103</a></li>
                                <li class="ampstart-dropdown-item"><a href="@($"{Model?.PageCanonicalUrl}/microsoft/az-900")" class="text-decoration-none">AZ-900</a></li>
                            </ul>
                        </section>
                    </amp-accordion>
                    <!-- End Dropdown-inline -->
                </li>
                <li class="ampstart-nav-item land-see-sidebar-nav-item ampstart-title-sm bold"><a class="ampstart-nav-link" href="@($"{Model?.PageCanonicalUrl}/salesforce")">Salesforce</a></li>
                <li class="ampstart-nav-item ampstart-nav-dropdown relative ">

                    <!-- Start Dropdown-inline -->
                    <amp-accordion layout="container" disable-session-states="" class="ampstart-dropdown">
                        <section expanded="">
                            <header>Salesforce</header>
                            <ul class="ampstart-dropdown-items list-reset m0 p0">
                                <li class="ampstart-dropdown-item"><a href="@($"{Model?.PageCanonicalUrl}/salesforce/crt-251")" class="text-decoration-none">CRT-251</a></li>
                            </ul>
                        </section>
                    </amp-accordion>
                    <!-- End Dropdown-inline -->
                </li>
                <li class="ampstart-nav-item land-see-sidebar-nav-item ampstart-title-sm bold"><a class="ampstart-nav-link" href="#">About</a></li>
                <li class="ampstart-nav-item land-see-sidebar-nav-item ampstart-title-sm bold"><a class="ampstart-nav-link" href="#">Contact</a></li>

            </ul>
        </nav>
        <ul class="ampstart-social-follow list-reset flex justify-around items-center flex-wrap m0 mb4">
            <li>
                <a href="https://www.facebook.com/ExamAnswerCom" target="_blank" class="inline-block p1" aria-label="Link to AMP HTML Facebook"><svg xmlns="http://www.w3.org/2000/svg" width="24" height="23.6" viewBox="0 0 56 55"><title>Facebook</title><path d="M47.5 43c0 1.2-.9 2.1-2.1 2.1h-10V30h5.1l.8-5.9h-5.9v-3.7c0-1.7.5-2.9 3-2.9h3.1v-5.3c-.6 0-2.4-.2-4.6-.2-4.5 0-7.5 2.7-7.5 7.8v4.3h-5.1V30h5.1v15.1H10.7c-1.2 0-2.2-.9-2.2-2.1V8.3c0-1.2 1-2.2 2.2-2.2h34.7c1.2 0 2.1 1 2.1 2.2V43" class="ampstart-icon ampstart-icon-fb"></path></svg></a>
            </li>
        </ul>

    </amp-sidebar>
    <!-- End Sidebar -->
    <!-- End Navbar -->

    @RenderBody()

    <!-- Start Footer -->
    <footer class="ampstart-footer flex flex-column items-center mx-auto max-width-3 ">


        <ul class="ampstart-social-follow list-reset flex justify-around items-center flex-wrap m0 mb4">
            <li>
                <a href="https://www.facebook.com/ExamAnswerCom" target="_blank" class="inline-block p1" aria-label="Link to AMP HTML Facebook"><svg xmlns="http://www.w3.org/2000/svg" width="24" height="23.6" viewBox="0 0 56 55"><title>Facebook</title><path d="M47.5 43c0 1.2-.9 2.1-2.1 2.1h-10V30h5.1l.8-5.9h-5.9v-3.7c0-1.7.5-2.9 3-2.9h3.1v-5.3c-.6 0-2.4-.2-4.6-.2-4.5 0-7.5 2.7-7.5 7.8v4.3h-5.1V30h5.1v15.1H10.7c-1.2 0-2.2-.9-2.2-2.1V8.3c0-1.2 1-2.2 2.2-2.2h34.7c1.2 0 2.1 1 2.1 2.2V43" class="ampstart-icon ampstart-icon-fb"></path></svg></a>
            </li>
            <li>
                <a href="#" target="_blank" class="inline-block p1" aria-label="Link to AMP HTML E-mail"><svg xmlns="http://www.w3.org/2000/svg" width="24" height="18.4" viewBox="0 0 56 43"><title>email</title><path d="M10.5 6.4C9.1 6.4 8 7.5 8 8.9v21.3c0 1.3 1.1 2.5 2.5 2.5h34.9c1.4 0 2.5-1.2 2.5-2.5V8.9c0-1.4-1.1-2.5-2.5-2.5H10.5zm2.1 2.5h30.7L27.9 22.3 12.6 8.9zm-2.1 1.4l16.6 14.6c.5.4 1.2.4 1.7 0l16.6-14.6v19.9H10.5V10.3z" class="ampstart-icon ampstart-icon-email"></path></svg></a>
            </li>
        </ul>
        <nav class="ampstart-footer-nav">
            <ul class="list-reset flex flex-wrap mb3">
                <li class="px1"><a class="text-decoration-none ampstart-label" href="#">About</a></li>
                <li class="px1"><a class="text-decoration-none ampstart-label" href="#">Contact</a></li>
            </ul>
        </nav>
        <small>
            © Exam Answer, @(DateTime.UtcNow.Year) •
            <a class="text-decoration-none" href="@($"{Model?.PageCanonicalUrl}/privacy")">Privacy Policy</a>
        </small>
    </footer>
    <!-- End Footer -->
</body>
</html>