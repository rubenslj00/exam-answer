@{
    ViewData["title"] = "Exam AZ-100: Microsoft Azure Infrastructure and Deployment.";
    ViewData["description"] = "Prepare for Exam AZ-100: Microsoft Azure Infrastructure and Deployment. Free demo questions with answers and explanations.";
}
<div class="k-block k-info-colored" style="text-align: center;">
    <div class="k-header k-info-colored">
        <h1>
            @ViewData["title"]
        </h1>
    </div>
    <h2>
        @ViewData["description"]
    </h2>
</div>
@{
    for (int i = 1; i <= 115; i++)
    {
        <br />
        <partial name="~/views/microsoft/az-100/question@(i).cshtml" model="@new QuestionViewModel() { Id = i.ToString() }" />
    }
}